<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.easy_attendance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Easy_Attendance">
        <activity android:name=".StartingAnimation"
            android:theme="@style/Theme.Easy_Attendance.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Record" />
        <activity android:name=".ForgotPassword"
            android:theme="@style/Theme.Easy_Attendance.NoActionBar"/>
        <activity
            android:name=".RegisterUser"
            android:theme="@style/Theme.Easy_Attendance.NoActionBar" />
        <activity
            android:name=".LogIn"
            android:theme="@style/Theme.Easy_Attendance.NoActionBar">

        </activity>
        <activity android:name=".Result"
            android:theme="@style/Theme.Easy_Attendance.NoActionBar"/>
        <activity android:name=".Attendance"
            android:theme="@style/Theme.Easy_Attendance.NoActionBar"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Easy_Attendance.NoActionBar"
        />
    </application>

</manifest>